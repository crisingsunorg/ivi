{
  "name": "ivi",
  "version": "0.15.0",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "es2016": "dist/es2016/index.js",
  "typings": "dist/typings/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "!src/**/__tests__",
